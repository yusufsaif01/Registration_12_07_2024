{
	"name": {
		"doc": "Application name .",
		"format": "String",
		"default": "YFTChain",
		"env": "APP_NAME",
		"arg": "app_name"
	},
	"version": {
		"doc": "Version of application",
		"format": "String",
		"default": "1.0.0",
		"env": "APP_VERSION",
		"arg": "app_version"
	},
	"baseURL": {
		"doc": "For setting down the base url of app.",
		"format": "url",
		"default": "https://test.yftchain.com/",
		"env": "APP_BASE_URL",
		"arg": "app_base_url"
	},
	"environment": {
		"doc": "environment (dev, prod)",
		"format": "String",
		"default": "dev",
		"env": "APP_ENVIRONMENT",
		"arg": "app_environment"
	},
	"client_domains":{
		"doc": "For setting down the client urls of app.",
		"default": "https://test.yftchain.com,http://localhost,https://localhost,http://localhost:4200",
		"env": "APP_CLIENT_URLS",
		"arg": "app_client_urls"
	},
	"facebook_url": {
        "doc": "provide default facebook url",
        "format": "String",
        "default": "https://www.facebook.com/YFTCHAIN",
        "env": "FACEBOOK_URL",
        "arg": "facebook_url"
    },
	"instagram_url": {
        "doc": "provide default instagram url",
        "format": "String",
        "default": "https://www.instagram.com/yftchain/",
        "env": "INSTAGRAM_URL",
        "arg": "instagram_url"
    },
	"linkedin_url": {
        "doc": "provide default linkedin url",
        "format": "String",
        "default": "https://www.linkedin.com/company/decodingyouthtalent/",
        "env": "LINKEDIN_URL",
        "arg": "linkedin_url"
    },
	"twitter_url": {
        "doc": "provide default twitter url",
        "format": "String",
        "default": "#",
        "env": "TWITTER_URL",
        "arg": "twitter_url"
    },
	"privacy_policy_url": {
        "doc": "provide default twitter url",
        "format": "String",
        "default": "http://yftchain.com/privacy-policy",
        "env": "PRIVACY_POLICY_URL",
        "arg": "privacy_policy_url"
    },
	"terms_and_conditions_url": {
        "doc": "provide default twitter url",
        "format": "String",
        "default": "http://yftchain.com/terms&conditions",
        "env": "TERMS_AND_CONDITIONS_URL",
        "arg": "terms_and_conditions_url"
    },
	"contact_us_url": {
        "doc": "provide default twitter url",
        "format": "String",
        "default": "#",
        "env": "CONTACT_US_URL",
        "arg": "contact_us_url"
    },
	"contact_us_email": {
        "doc": "provide default twitter email",
        "format": "String",
        "default": "admin@yftchain.com",
        "env": "CONTACT_US_EMAIL",
        "arg": "contact_us_email"
    },
	"restricted_access": {
        "doc": "restricts access on backend server",
        "format": "Boolean",
        "default": "false",
        "env": "RESTRICTED_ACCESS",
        "arg": "restricted_access"
    }
}
